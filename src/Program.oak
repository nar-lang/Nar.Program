module Oak.Program

alias native Program[flags, model, view, msg]

def native application(lifecycle:
  { init : (flags): ( model, Cmd[msg] )
  , view : (model): view
  , present: (view): ()
  , update : (msg, model): ( model, Cmd[msg] )
  , subscriptions : (model): Sub[msg]
  }
): Program[flags, model, msg]

def native execute(program: Program[List[String], model, msg]): Int
